

type Frete{
    id:ID
    custo:String
    nota:String
    nome:String



}
input FreteInput{
    custo:String
    nota:String
    nome:String


}
input FreteUpdatedInput{
    id:ID
    custo:String
    nota:String
    nome:String


}

extend type Query {

    frete(id:ID!):Frete

    fretes:[Frete]!

}

extend type Mutation {

    save(frete:FreteInput!):Frete

    updated(frete:FreteUpdatedInput!):Frete

    removerFrete(id:ID!):Boolean
}